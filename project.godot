[application]

config/name="Volumetrics Plugin Demo"
config/description="This project demonstrates what you can be done with the Volumetrics Plugin."
run/main_scene="res://demo/volumetrics_demo.tscn"
config/icon="res://demo/icon.png"

[autoload]

VolumetricServer="*res://addons/silicon.vfx.volumetrics/volumetric_server.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "silicon.vfx.volumetrics" )

[importer_defaults]

texture_3d={
"compress/mode": 0,
"compress/no_bptc_if_rgb": false,
"flags/filter": true,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 1,
"slices/horizontal": 8,
"slices/vertical": 16
}

